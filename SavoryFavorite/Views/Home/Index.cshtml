@{
    Layout = null;
}
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <link rel="shortcut icon" href="/favicon.ico" />
    <title>网站收藏夹管理后台</title>
    <link href="lib/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" />
    <link href="lib/bootstrap/3.3.7/css/bootstrap-theme.min.css" rel="stylesheet" />
    <link href="lib/angular-ui-bootstrap/2.5.0/css/ui-bootstrap-csp.css" rel="stylesheet" />
    <link href="lib/jquery-ui/1.12.1/css/jquery-ui.min.css" rel="stylesheet" />
    <link href="lib/sweetalert/1.1.3/sweetalert.min.css" rel="stylesheet" />
    <link href="lib/icheck-bootstrap/3.0.1/icheck-bootstrap.min.css" rel="stylesheet" />
    <link href="styles/style.css" rel="stylesheet" />
</head>

<body ng-app="app">

    <div ui-view="header"></div>

    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-1" ui-view="main-menu"></div>
            <div class="col-sm-11" ui-view="main-body"></div>
        </div>
    </div>

    <script src="lib/jquery/1.10.2/jquery.min.js"></script>
    <script src="lib/jquery-ui/1.12.1/js/jquery-ui-1.12.1.min.js"></script>
    <script src="lib/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="lib/angularjs/1.7.3/angular.min.js"></script>
    <script src="lib/angular-ui-router/1.0.5/angular-ui-router.min.js"></script>
    <script src="lib/angular-ui-sortable/0.19.0/angular-ui-sortable.min.js"></script>
    <script src="lib/angularjs/1.7.3/angular-resource.min.js"></script>
    <script src="lib/angular-ui-bootstrap/2.5.0/js/ui-bootstrap-tpls.min.js"></script>
    <script src="lib/sweetalert/1.1.3/sweetalert.min.js"></script>

    <script src="scripts/_route.js?v=@ViewBag.ReleaseNo"></script>
    <script src="scripts/_service.js?v=@ViewBag.ReleaseNo"></script>
    <script src="scripts/controller/HeaderController.js?v=@ViewBag.ReleaseNo"></script>
    <script src="scripts/controller/FavoriteListController.js?v=@ViewBag.ReleaseNo"></script>
    <script src="scripts/controller/FavoriteItemController.js?v=@ViewBag.ReleaseNo"></script>
    <script src="scripts/controller/FavoriteCreateController.js?v=@ViewBag.ReleaseNo"></script>
    <script src="scripts/controller/FavoriteUpdateController.js?v=@ViewBag.ReleaseNo"></script>
    <script src="scripts/controller/CategoryListController.js?v=@ViewBag.ReleaseNo"></script>
    <script src="scripts/controller/CategoryItemController.js?v=@ViewBag.ReleaseNo"></script>
    <script src="scripts/controller/CategoryCreateController.js?v=@ViewBag.ReleaseNo"></script>
    <script src="scripts/controller/CategoryUpdateController.js?v=@ViewBag.ReleaseNo"></script>
    <script src="scripts/_app.js?v=@ViewBag.ReleaseNo"></script>

    <script>
        window.releaseNo = "@ViewBag.ReleaseNo";
    </script>

</body>

</html>
